{
	"name": "@kazu80/clubs-plugin-links",
	"version": "1.0.1",
	"type": "module",
	"description": "This is a links plugin for Clubs",
	"main": "dist/index.js",
	"exports": {
		".": {
			"default": "./dist/index.js"
		}
	},
	"types": "./dist/src/index.d.ts",
	"files": [
		"dist/**/*.d.ts",
		"dist/**/*.js",
		"!**/*.test.*",
		"src"
	],
	"keywords": [
		"clubs-plugin"
	],
	"scripts": {
		"preview": "astro --root preview dev",
		"build": "rollup -c",
		"pretest": "yarn build",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js --fix",
		"lint:format": "prettier --write '**/*.{ts,js,json,md,yml}'",
		"prepack": "yarn test && yarn build",
		"prepare": "husky install",
		"coverage": "vitest run --coverage",
		"test": "vitest",
		"test:ui": "vitest --ui"
	},
	"author": "Kazuyoshi Kawakami <saruyama.monki@gmail.com> (https://github.com/kazu80)",
	"license": "MPL-2.0",
	"dependencies": {
		"@devprotocol/dev-kit": "7.3.2",
		"@devprotocol/util-ts": "3.0.0",
		"bignumber.js": "9.1.1",
		"ethers": "5.7.2",
		"ramda": "0.28.0",
		"sass": "1.60.0"
	},
	"devDependencies": {
		"@astrojs/svelte": "^2.1.0",
		"@astrojs/tailwind": "3.1.1",
		"@astrojs/vue": "2.1.0",
		"@ava/typescript": "4.0.0",
		"@devprotocol/clubs-core": "^3.0.0-alpha.0",
		"@rollup/plugin-image": "^3.0.2",
		"@rollup/plugin-typescript": "11.0.0",
		"@types/dotenv": "8.2.0",
		"@types/node": "18.15.8",
		"@types/ramda": "0.28.23",
		"@typescript-eslint/eslint-plugin": "5.56.0",
		"@typescript-eslint/parser": "5.56.0",
		"@vitejs/plugin-vue": "^4.1.0",
		"@vitest/coverage-c8": "^0.30.1",
		"@vitest/ui": "^0.30.1",
		"@vue/test-utils": "^2.3.2",
		"astro": "^2.1.0",
		"ava": "5.2.0",
		"dotenv": "16.0.3",
		"eslint": "8.36.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-functional": "4.4.1",
		"happy-dom": "^9.2.1",
		"husky": "8.0.3",
		"prettier": "2.8.7",
		"prettier-plugin-astro": "^0.8.0",
		"prettier-plugin-svelte": "^2.9.0",
		"rimraf": "4.4.1",
		"rollup": "3.20.2",
		"svelte": "^3.54.0",
		"tailwindcss": "3.2.7",
		"typescript": "5.0.2",
		"vitest": "^0.30.1",
		"vue": "3.2.47"
	},
	"peerDependencies": {
		"@devprotocol/clubs-core": "^2.0.0 || ^3.0.0"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			},
			"compile": false
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kazu80/clubs-links.git"
	},
	"bugs": {
		"url": "https://github.com/kazu80/clubs-links/issues"
	},
	"homepage": "https://github.com/kazu80/clubs-links#readme"
}
